songs/* filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
images/*.jpg filter=lfs diff=lfs merge=lfs -text
images/*.png filter=lfs diff=lfs merge=lfs -text
images/* filter=lfs diff=lfs merge=lfs -text
images/*.jpg !text !filter !merge !diff
images/*.jpeg !text !filter !merge !diff
images/*.png !text !filter !merge !diff
images/*.webp !text !filter !merge !diff
